<div class="card">
  <div class="card-block">
    <div class="row">
      <div class="col-md-1 chat-avatar">
       <!-- <%= message.user.profile.avatar.url %> -->
      </div>
      <div class="col-md-11">
        <p class="card-text">
          <span class="text-muted"><%= message.user.profile.first_name %>, <%= message.user.profile.last_name %> at <%= message.timestamp %> says</span></br>
          <%= message.body %>
        </p>
      </div>
    </div>
  </div>
</div>